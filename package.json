{
  "name": "gonest",
  "version": "1.0.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Lightweight backend decorators for Express",
  "exports": {
    ".": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Asad-Ali-Developer/Gonest.git"
  },
  "keywords": [
    "nesty",
    "backend",
    "express",
    "decorators",
    "lightweight"
  ],
  "author": "Modest",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "private": false,
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc && dts-bundle-generator --no-check -o dist/index.d.ts src/index.ts",
    "prepublishOnly": "npm run build",
    "postinstall": "ts-node-dev --clear --respawn --transpile-only --watch src src/index.ts",
    "dev": "ts-node-dev --clear --respawn --transpile-only --watch src src/index.ts"
  },
  "peerDependencies": {
    "ts-node-dev": "^2.0.0"
  },
  "dependencies": {
    "colorette": "^2.0.20",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "express": "^4.21.2",
    "mongoose": "^8.10.0",
    "prettier": "^3.5.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.5",
    "socket.io": "^4.8.1",
    "tsc-watch": "^6.2.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.1",
    "dts-bundle-generator": "^9.5.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}